        <Border Grid.Column="0" Grid.Row="2" BorderBrush="#DDDDDD" BorderThickness="0,2,0,0" Height="60" x:Name="BdConfirm">
            <Grid >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
