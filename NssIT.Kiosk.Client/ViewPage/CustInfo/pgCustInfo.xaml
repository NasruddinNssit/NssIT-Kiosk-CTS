                            <TextBox BorderThickness="0" Foreground="#FF333333" VerticalContentAlignment="Center" FontSize="15" LostFocus="TextBox_LostFocus" Padding="0 0 0 4" MaxLines="1" MaxLength="14" GotFocus="TextBoxData_GotFocus" x:Name="TxtPassId19">
