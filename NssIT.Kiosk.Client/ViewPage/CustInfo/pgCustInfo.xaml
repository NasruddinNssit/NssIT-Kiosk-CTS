                <Button Grid.Column="1" Grid.Row="0" Grid.RowSpan="2"  Style="{StaticResource ConfirmButton}" Content="{DynamicResource CONFIRM_Label}" FontSize="16" Height="45" Click="Confirm_Click" x:Name="BtnConfirm"/>

