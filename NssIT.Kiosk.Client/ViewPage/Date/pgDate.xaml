<Page x:Class="NssIT.Kiosk.Client.ViewPage.Date.pgDate"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      xmlns:local="clr-namespace:NssIT.Kiosk.Client.ViewPage.Date"
     mc:Ignorable="d" 
      d:DesignHeight="900" d:DesignWidth="900"
      FontFamily="Segoe UI"
      Stylus.IsPressAndHoldEnabled="False"
      Title="Date Selection" Background="White" Loaded="Page_Loaded">

    <!--d:DesignHeight="710" d:DesignWidth="750"-->
    <!--d:DesignHeight="1024" d:DesignWidth="1280"-->
    <!--d:DesignHeight="1920" d:DesignWidth="1080"-->

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="rosDateEnglish.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="40"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="40"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="10" />
            <RowDefinition Height="150" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Grid.Resources>
            <!--<ResourceDictionary>
                <sys:Double x:Key="circle_Day">46</sys:Double>
                <sys:Double x:Key="halfBlock_Day">45</sys:Double>
                <sys:Double x:Key="topBlock_Day">4</sys:Double>
                <sys:Double x:Key="bottomBlock_Day">4</sys:Double>
                <sys:Double x:Key="fontSize_Day">20</sys:Double>
            </ResourceDictionary>-->
            <ResourceDictionary>
                <sys:Double x:Key="circle_Day">75</sys:Double>
                <sys:Double x:Key="halfBlock_Day">56</sys:Double>
                <sys:Double x:Key="topBlock_Day">8</sys:Double>
                <sys:Double x:Key="bottomBlock_Day">8</sys:Double>
                <sys:Double x:Key="fontSize_Day">27</sys:Double>
            </ResourceDictionary>
        </Grid.Resources>

        <Border Grid.Column="1" Grid.Row="0" Height="0" x:Name="BdStacker1"/>
        
        <Grid x:Name="GrdDateSelection" Grid.Column="1" Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="30"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="30"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="90" />
                <RowDefinition Height="10" />
                <RowDefinition Height="40" />

                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
                <RowDefinition Height="90" />
            </Grid.RowDefinitions>

            <StackPanel Grid.Column="1" Grid.Row="0" Orientation="Vertical" MouseDown="PreviousMonth_MouseDown" Background="#00FFFFFF" x:Name="SkpPrevious">
                <TextBlock Text="" Margin="0,5,0,0"/>
                <Image Source="/NssIT.Kiosk.Client;component/Resources/icon-chevron-left-light.png" Height="45" Margin="35,0,35,0" Stretch="Fill" RenderTransformOrigin="0.5,0.5" >
                    <Image.RenderTransform>
                        <TransformGroup>
                            <RotateTransform Angle="0"/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <TextBlock Text="{DynamicResource PREVLabel}" Margin="0,5,0,0" Foreground="Black" HorizontalAlignment="Center"  Width="70" TextAlignment="Center" Height="22" FontSize="16" />
            </StackPanel>
            <TextBlock x:Name="TxtMonthYearTag" Grid.Column="2" Grid.Row="0" Grid.ColumnSpan="5" FontSize="22" Text="JUNE 2020" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FF444444" FontWeight="Bold" />
            <StackPanel Grid.Column="7" Grid.Row="0" Orientation="Vertical" MouseDown="NextMonth_MouseDown" Background="#00FFFFFF" x:Name="SkpNext">
                <TextBlock Text="" Margin="0,5,0,0"/>
                <Image Source="/NssIT.Kiosk.Client;component/Resources/icon-chevron-left-light.png" Height="45" Margin="35,0,35,0" Stretch="Fill" RenderTransformOrigin="0.5,0.5" >
                    <Image.RenderTransform>
                        <TransformGroup>
                            <RotateTransform Angle="180"/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <TextBlock Text="{DynamicResource NEXTLabel}" Margin="0,5,0,0" Foreground="Black" HorizontalAlignment="Center" Width="70" TextAlignment="Center" Height="22" FontSize="16" />
            </StackPanel>

            <TextBlock x:Name="TxtWeekDayTagX00" Grid.Column="1" Grid.Row="2" Text="MON" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="Black"  />
            <TextBlock x:Name="TxtWeekDayTagX01" Grid.Column="2" Grid.Row="2" Text="MON" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="22" Foreground="Black" />
